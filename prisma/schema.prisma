generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Record {
  id        Int      @id @default(autoincrement())
  artist    String
  title     String
  coverUrl  String?
  year      Int?
  genre     String?
  discogsId Int?
  aiVibe    String?
  createdAt DateTime @default(now()) @index
}