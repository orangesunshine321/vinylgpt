{
  "name": "vinylgpt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx prisma db push && next dev -p 3000",
    "build": "next build",
    "start": "npx prisma db push && next start -p 3000",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "docker:up": "docker-compose up -d --build",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "update:deps": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "prisma": "^5.0.0",
    "@prisma/client": "^5.0.0",
    "openai": "^4.0.0",
    "axios": "^1.3.0",
    "next-pwa": "^5.6.0",
    "multer": "1.4.5-lts.1",
    "json2csv": "^5.0.7",
    "next-connect": "^0.12.2",
    "tesseract.js": "^4.0.3"
  },
  "devDependencies": {
    "typescript": "^5.1.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^13.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/multer": "^1.4.7",
    "npm-check-updates": "^16.0.0"
  }
}
